        int n,temp=0;
        printf("Enter the number n: ");
        scanf("%d",&n);
        for(int i=2; i<=n/2; i++)
        {
            if(n%i==0)
            {
               temp++; 
            }
        }
        if(temp==0)
        {
            printf("Number is prime: ");
        }
        else
        {
            printf("Number is not prime");
        }
