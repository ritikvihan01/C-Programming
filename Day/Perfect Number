        int n,sum=0;
        printf("Enter the number: ");
        scanf("%d",&n);
        int temp=n;
        for(int i=1; i<n; i++)
        {

            if(n%i==0)
            {
                sum=sum+i;
            }
        }
        if(temp==sum)
        {
            printf("Number is perfect: ");
        }
        else
        {
            printf("Number is not perfect: ");
        }
