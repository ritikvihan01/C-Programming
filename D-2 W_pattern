        for(int i=1; i<=6; i++)
        {
            for(int j=1; j<=15; j++)
            {
                if(i==j||i==16-j||(i==5&&j==7)||(i==4&&j==8)||(i==5&&j==9))
                {
                    printf("*");
                }
                else
                {
                    printf(" ");
                }
            }
            
            printf("\n");
        }
